html
	head
		title Log in 
			
	body

		include ../partials/head

		div#main
			h2 Sign In
			
			form(action='/login' method='post' target='_blank')
				label UserName
				br
				input#username(type='text' name='username')
				br
				label Password
				br
				input#password(type='text' name='password')
				br
				input(type='submit' value='submit' onclick="self.close()")



		